@section Scripts{
    <script src="~/Scripts/Home/Index.js"></script>
    <script id="datawomensslider" type="text/x-jsrender">
        <div class="item">
            <input type="hidden" id="txt_collectionkeyword{{>woman_collection_id}}" value="{{>collectionkeyword}}" />
            <input type="hidden" id="txt_subcategoryname{{>woman_collection_id}}" value="{{>subcategoryname}}" />
