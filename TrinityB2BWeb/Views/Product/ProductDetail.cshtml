    <div class="modal right fade" id="modalcustomize" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="z-index:9999">
        <div class="modal-dialog side-pwdth" role="document">
            <div class="modal-content">
                <div class="modal-header" style="padding: 10px 15px;">
                    <button type="button" class="close addscaleclass" data-dismiss="modal" aria-label="Close" id="closecustomizemodal" style="font-size: 32px"><span aria-hidden="true">&times;</span></button>
                    <h3 class="modal-title" id="myModalLabel" style="font-size: 18px;color: #c75534;">Total Price:<span id="showtotalprice"></span></h3>
                </div>
                <div class="modal-body" style="padding-top:0px">
                    <div class="list-group">
                        <input type="hidden" id="hidenshapename" />
                        <div style="padding:10px 5px;">
                            <form style="border-radius:2px;">
                                <div class="row form-group ptagfont">
                                    <p>Gold Purity : <span class="final-select" id="finalgoldpurity"></span></p>
                                    <div>
                                        <ul class="scrollmenu cursorcss" style="display: inline-flex;" id="txtgoldpurity"></ul>
                                    </div>
                                </div>
                                <div class="row form-group ptagfont">
                                    <p>Gold Color : <span class="final-select" id="finalgoldcolor"></span></p>
                                    <div>
                                        <ul class="scrollmenu cursorcss" style="display: inline-flex;" id="txtgoldcolor"></ul>
                                    </div>
                                </div>
                                <div class="row form-group ptagfont">
                                    <p>Diamond Clarity : <span class="final-select" id="finaldiamonpurity"></span></p>
                                    <div style="overflow-x:auto" class="scrollviewsize">
                                        <ul class="scrollmenu cursorcss" style="display: inline-flex;" id="txtdiamonclarity"></ul>
                                    </div>
                                </div>
                                <div class="row form-group ptagfont">
                                    <p>Diamond Color : <span class="final-select" id="finaldiamoncolor"></span></p>
                                    <div>
                                        <ul class="scrollmenu cursorcss" style="display: inline-flex;" id="txtdiamoncolor"></ul>
                                    </div>
                                </div>
                                <div class="row form-group ptagfont" id="cutomizesize" style="display:none">
                                    <p>Size : <span id="finaldesignsize"></span></p>
                                    <div style="overflow-x:auto;padding-left:0;" class="col-md-4 col-xs-12">
                                        <select id="txtdesignsize" class="form-control" onchange="SelectedDesignsie()"></select>
                                        @*<ul class="scrollmenu" style="display: inline-flex;" id="txtdesignsize"></ul>*@
                                    </div>
                                    <div>
                                        <img id="lbl_SizeCountry" src="" style="width: 30px;margin-top: 2px;" />
                                    </div>
                                </div>
                                <div class="row form-group ptagfont">
                                    <p>Customize Note(s) :</p>
                                    <textarea class="form-control" rows="4" placeholder="Write special notes here.." id="txtcustomizenotes"></textarea>
                                </div>

                                <div class="row applyaddcart" style="width:100%;margin-top:10px">
                                    <div class="col-sm-6 col-xs-6">
                                        <button type="button" class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20 backcartcolor" id="btnaddapplycart">Apply / Add to cart</button>
                                    </div>
                                    <div class="col-sm-6 col-xs-6">
                                        <button type="button" class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20 dltbackground" id="btncanclecustomize">Cancel</button>
                                    </div>
                                </div>
                            </form>

                        </div>
                        <div>
                        </div>
                    </div>
                </div>

            </div><!-- modal-content -->
        </div><!-- modal-dialog -->
    </div>

    <div class="modal right fade" id="modalEmail" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="z-index:9999">
        <div class="modal-dialog side-pwdth" role="document">
            <div class="modal-content">
                <div class="modal-header" style="padding: 10px 15px;">
                    <button type="button" class="close addscaleclass" data-dismiss="modal" aria-label="Close" id="close_modalEmail" style="font-size: 32px"><span aria-hidden="true">&times;</span></button>
                    <h3 class="modal-title" style="font-size: 18px;color: #c75534;">Share To Mail :</h3>
                </div>
                <div class="modal-body" style="padding-top:0px">
                    <div class="list-group">
                        <div style="padding:10px 5px;">
                            <form id="frm_shareEmail" style="border-radius:2px;">
                                <div class="row form-group">
                                    <p>To Email :</p>
                                    <input type="text" class="form-control required" placeholder="Enter email id ..." id="txt_toEmail" name="txt_toEmail" />
                                </div>
                                <div class="row form-group">
                                    <p>Subject :</p>
                                    <input type="text" class="form-control required" placeholder="Enter Subject of email ..." id="txt_Subject" name="txt_Subject" value="Design @ViewBag.Designno shared by TrinityJewells" />
                                </div>
                                <div class="row form-group">
                                    <p>Email Body :</p>
                                    @*<textarea class="form-control required" rows="4" placeholder="Enter body of mail here.." id="txt_mailBody" name="txt_mailBody" value=""></textarea>*@
                                    <div id="txt_mailBody" class="summernote"></div>
                                    <span style="font-style:italic;font-size:11px;color:red;"><b>Note : </b>{SHARE URL} replaced automatically with actual data. Do not change it.</span>
                                </div>

                                <div class="row form-group">
                                    <button type="button" class="btn btn-success btn-md" id="btn_sendEmail">Send &nbsp;&nbsp;<i class="fa fa-paper-plane-o"></i></button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div><!-- modal-content -->
        </div><!-- modal-dialog -->
    </div>

    <div class="modal right fade" id="modalSMS" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="z-index:9999">
        <div class="modal-dialog side-pwdth" role="document">
            <div class="modal-content">
                <div class="modal-header" style="padding: 10px 15px;">
                    <button type="button" class="close addscaleclass" data-dismiss="modal" aria-label="Close" style="font-size: 32px"><span aria-hidden="true">&times;</span></button>
                    <h3 class="modal-title" style="font-size: 18px;color: #c75534;">Share To SMS :</h3>
                </div>
                <div class="modal-body" style="padding-top:0px">
                    <div class="list-group">
                        <div style="padding:10px 5px;">
                            <form id="frm_shareSMS" style="border-radius:2px;">
                                <div class="row form-group">
                                    <p>Mobile No :</p>
                                    <input type="text" class="form-control required" placeholder="Enter mobile number ..." id="txt_MobileNo" name="txt_MobileNo" />
                                </div>
                                <div class="row form-group">
                                    <p>SMS Body :</p>
                                    <textarea class="form-control required" rows="4" placeholder="Enter body of SMS here.." id="txt_SMSBody" name="txt_SMSBody"></textarea>
                                    <span style="font-style:italic;font-size:11px;color:red;"><b>Note : </b>{SHARE URL} replaced automatically with actual data. Do not change it.</span>
                                </div>

                                <div class="row form-group">
                                    <button type="button" class="btn btn-success btn-md" id="btn_sendSMS">Send &nbsp;&nbsp;<i class="fa fa-paper-plane-o"></i></button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div><!-- modal-content -->
        </div><!-- modal-dialog -->
    </div>

    <div class="modal right fade" id="modalWhatsapp" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="z-index:9999">
        <div class="modal-dialog side-pwdth" role="document">
            <div class="modal-content">
                <div class="modal-header" style="padding: 10px 15px;">
                    <button type="button" class="close addscaleclass" data-dismiss="modal" aria-label="Close" id="close_modalWhatsapp" style="font-size: 32px"><span aria-hidden="true">&times;</span></button>
                    <h3 class="modal-title" style="font-size: 18px;color: #c75534;">Share To WhatsApp :</h3>
                </div>
                <div class="modal-body" style="padding-top:0px">
                    <div class="list-group">
                        <div style="padding:10px 5px;">
                            <form id="frm_shareWhatsapp" style="border-radius:2px;">
                                <div class="row form-group">
                                    <p>Message :</p>
                                    <textarea class="form-control required" rows="4" placeholder="Enter the message here.." id="txt_WAMessage" name="txt_WAMessage"></textarea>
                                    <span style="font-style:italic;font-size:11px;color:red;"><b>Note : </b>{SHARE URL} replaced automatically with actual data. Do not change it.</span>
                                </div>

                                <div class="row form-group">
                                    <button type="button" class="btn btn-success btn-md" id="btn_sendWhatsapp">Send &nbsp;<i class="fa fa-paper-plane-o"></i></button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div><!-- modal-content -->
        </div><!-- modal-dialog -->
    </div>

    @if (TrinityB2BWeb.CommonClasses.SessionFacade.UserSession.USERTYPE != "Party")
    {
        <div class="icon-bar">
            <a href="javascript:void(0);" class="mail" id="icon_shareToMail" title="Share To Email"><i class="fa fa-envelope-o"></i></a>
            <a href="javascript:void(0);" class="sms" id="icon_shareToSMS" title="Share To SMS"><i class="fa fa-comment-o"></i></a>
            <a href="javascript:void(0);" class="whatsapp" id="icon_shareToWhatsApp" title="Share To Whatsapp"><i class="fa fa-whatsapp"></i></a>
        </div>
    }

    <input type="hidden" value="@HttpContext.Current.Request.Url.Authority@Url.Action("SharedProductDetail","Login")?id=@ViewBag.EncryptedDesignno&stocktype=@ViewBag.Stocktype" id="hdn_ShareLink" />

    <input type="hidden" value="@TrinityB2BWeb.CommonClasses.SessionFacade.UserSession.CONTACTPERSONNAME" id="hdn_UserName" />
</div>
@section Scripts{
