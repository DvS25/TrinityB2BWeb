@{
    Layout = "~/Views/Shared/_SecondLayout.cshtml";
    ViewBag.Title = "Product Detail";
}
<style>
    .designnumberorange {
        margin: 0px !important;
        font-size: 21px;
        color: #c75534;
    }

    .topleftkt {
        margin-left: 10px;
        padding-left: 10px;
        margin-bottom: 5px;
        border-left: 1px solid #777575;
    }

    .icon-bar {
        position: fixed;
        top: 50%;
        -webkit-transform: translateY(-50%);
        -ms-transform: translateY(-50%);
        transform: translateY(-50%);
        right: 0px;
    }

        .icon-bar a {
            display: block;
            text-align: center;
            padding: 16px;
            transition: all 0.3s ease;
            color: white;
            font-size: 20px;
        }

            .icon-bar a:hover {
                background-color: #000;
            }

    .mail {
        background: #337ab7;
        color: white;
    }

    .sms {
        background: #dd4b39;
        color: white;
    }

    .whatsapp {
        background: #18ca11;
        color: white;
    }

    .note-editor .btn-default {
        color: #333 !important;
    }

        .note-editor .btn-default:hover, .note-editor .btn-default:active {
            color: #fff !important;
        }
</style>
<div class="container-fluid">
    <div class="product_title pr2">
        <h3 id="subcategoryname"></h3>
        <ul class="breadcrumb product_page  hidden-sm hidden-xs">
            <li><a href="@Url.Action("Index","Home")"><i class="fa fa-home"></i></a></li>
            <li><a href="@Url.Action("Index","Home")">Home</a></li>
            <li><a href="javascript:void(0)">Product Detail</a></li>
        </ul>
    </div>
    <input type="hidden" value="@ViewBag.Designno" id="hidedesignno" />
    <input type="hidden" value="@ViewBag.Stocktype" id="hideStocktype" />
    <input type="hidden" value="@ViewBag.GPURITY" id="hidegoldpurity" />
    <input type="hidden" value="@ViewBag.GCOLOR" id="hidegoldcolor" />
    <input type="hidden" value="@ViewBag.DPURITY" id="hidediamondpurity" />
    <input type="hidden" value="@ViewBag.DCOLOR" id="hidediamondcolor" />
    <input type="hidden" value="@ViewBag.DSIZE" id="hidedesignsize" />
    <input type="hidden" value="@ViewBag.CUSTUMIZENOTES" id="hidecutomizenotes" />
    <input type="hidden" value="@ViewBag.ISCUSTOMIZE" id="hideiscutomize" />
    <input type="hidden" value="@ViewBag.CARTID" id="hideiscutomizecartid" />
    <input type="hidden" value="@ViewBag.CARTTOPRODUCT" id="hidecarttoproduct" />

    <input type="hidden" id="metaltypevalue" value="" />
    <input type="hidden" id="productcustomizenotes" value="" />
    <div class="row" style="margin-bottom:50px;" id="productdetaildata">
        <div id="content" class="container mob-nopad content1">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 product_left mob-nopad" id="productimagecontent">
                  
                </div>

                @*<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 product_left mob-nopad" id="productimagecontent_rose">

                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 product_left mob-nopad" id="productimagecontent_yellow">

                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 product_left mob-nopad" id="productimagecontent_white">

                </div>*@
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 product_center mob-nopad">
                    <!--  <h2>Etiam Gravida></h2>-->
                    <div id="topdatabind"></div>
                    <div id="btnmoredetail">
                        <div class="row">
                            <div class="col-sm-6 mob-nopad" id="diamonddetails"></div>
                            <div class="col-sm-6 mob-nopad" id="golddetails"></div>
                        </div>
                        @*<div class="row" id="golddetails"></div>*@
                        <div class="row" id="colostonedetails"></div>
                        <div class="row">
                            <div class="col-sm-6 mob-nopad" id="makincharges"></div>
