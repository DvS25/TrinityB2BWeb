        <div class="related_product" id="divrelatedproduct" style="display:none;">
            <div class="box-header">
                <div class="row">
                    <div class="col-sm-12 col-xs-12">
                        <hr />
                        <div class="section-title">
                            <h1 class="fp_carousel-title">Related Designs   </h1>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="product-item owl-carousel owl-theme" id="related_product"></div>
            </div>
        </div>
